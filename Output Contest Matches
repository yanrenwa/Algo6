class Solution:
    """
    @param n: a integer, denote the number of teams
    @return: a string
    """
    def findContestMatch(self, n):
        # write your code here
        match = [""] * n
        for i in range(n):
            match[i] = str(i + 1)
     
        while n > 1:
            for i in range(n / 2):
                match[i] = "(" + match[i] + "," + match[n - 1 - i] + ")"
            n /= 2
        return match[0]
